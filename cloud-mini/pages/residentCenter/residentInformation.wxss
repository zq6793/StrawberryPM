.preview_panel{
  margin-top: 20rpx;
} 
.contract{
  width: 200rpx;
  height: 52rpx;
  line-height:52rpx;
  font-size: 26rpx;
  text-align: center;
  position: absolute;
  right: 30rpx;
  margin-top: 0;
  border:1px solid #614689;
  border-radius: 20rpx;
}
.other-deposit{
  color: rgb(243,64,53);
  border:1px solid rgb(243,64,53);
}
.view-contract{
  right: 250rpx;
  margin-top: 0;
}
.resident_center{
  height: 40rpx;
  line-height: 40rpx;
}
.resident_center:after{
   border-bottom: none;
}
.resident_color{
  color: #000;
}
.min_width{
  min-width:1em;
}
.resident_line{
  position: relative;
}
.resident_line::before{
  content:" ";
  position:absolute;
  top:0;
  right:0;
  height:1px;
  border-top:1rpx solid #d9d9d9;
  color:#d9d9d9;
  left:15px;
}
.resident_title{
  font-size: 30rpx;
}
.resident_photo{
  padding: 43rpx 30rpx;
}
.resident_distance{
  margin-bottom: 43rpx;
}
.resident_documents{
  box-sizing: border-box;
  height: 150rpx;
  width: 100%;
  text-align: center;
  padding: 0 40rpx;
} 
.img_distance{
  margin: 0 auto;
}
.resident_document{
  width:150rpx;
  height: 150rpx;
  text-align: center;
  line-height: 150rpx;
  border: 1rpx solid #d9d9d9;
} 
.resident_document image{
  width:150rpx;
  height: 150rpx;
}
.big_img{
  width: 53rpx;
  height:59rpx; 
}
.little_img{
  width:45rpx;
  height: 59rpx; 
}
.middle_img{
  width:66rpx;
  height: 59rpx; 
}
.equipment_dis{
  height: 78rpx;
  width: 172rpx;
  padding-top: 10rpx;
  box-sizing: border-box;
}
.parts{
  padding-left: 0px;
  padding-right: 0px;
}
.resident_equipment{
  text-align: center;
  padding: 45rpx 0rpx;
}
.distance_bottom{
  margin-bottom: 40rpx;
  display: flex;
}
.button-hover[type="primary"][plain]{
  background-color:transparent;
  color:rgba(88,55 ,138,0.6);
  border-color:rgba(88,55 ,138,0.6);
}
.password{
  color: #000;
  margin-left: 40rpx;
}