.code{
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 10;
  top: 0;
  left: 0;
  background: rgba(0,0,0,.5);
}
.code-cont{
  width: 50%;
  text-align: center;
  position: relative;
  top: 24%;
  left: 25%;
}
.code-img{
  width:100%;
  height:188px;
  border-bottom:1px #ededed solid;
}
.scan-code-end{
  color: #614689;
  font-size: 14px;
  height:54rpx;
  display:block;
}

.weui-form-preview::before,
.weui-cells::before{
  border-top: none;
}
.weui-form-preview__hd::after,
.weui-form-preview::after ,
.weui-cells::after{
  border-bottom: none;
} 