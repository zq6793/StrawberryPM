	text-align: left;
}
.pic-cont{
  width: 116rpx;
  height: 116rpx;
  border: 1px rgb(204,204,204) solid;
}
.pic-cont .weui-flex__item{
  margin: 0 10px 10px 0;
}
.pic-cont image{
  width: 116rpx;
  height: 116rpx;
}
.resident_documents{
  width:150rpx;
  height: 150rpx;
  text-align: center;
  line-height: 150rpx;
  border: 1rpx solid #d9d9d9;
}
.resident_documents image{
  width:150rpx;
  height: 150rpx;
}
.pic{
  padding: 0px 15px;
  margin-bottom: 10px;
}
.add_pic{
  width:150rpx;
  height: 150rpx;
  line-height: 150rpx;
  border: 1px #ededed solid;
  text-align: center;
  float:left;
}
.img_distances{
  margin-left: 20rpx;
}
.weui-check__hd_in-checkbox{
  margin-top: 0;
  position: absolute;
  right: 0;
}
.delete{
  width: 30rpx;
  height: 30rpx;
  line-height: 30rpx;
  position: absolute;
  right: 10px;
  margin-top: -22px;
  background: red;
  color: #fff;
  text-align: center;
  border-radius: 100%;
}

.bottom-flex{
  margin: 50rpx auto;
  text-align: center;
}
.weui-btn{
  width: 300rpx;
  height: 88rpx;
}
button[type="primary"]{
  background-color: #614789;
  border: none;
  color: #fff;
}
.weui-cell__ft{font-size: 28rpx;}
button[type="primary"][plain]{
  background-color: transparent;
  color: #614789;
  border: 1px solid #614789;
}
.button-hover[type="primary"][plain]{
  background-color:rgba(97,71,137,0.8);
  color: #614789;
  border: 1px solid #614789;
}
.check-out .weui-cell{
  height: 80rpx;
  line-height: 80rpx;
  padding:0 20rpx 0 30rpx;
}
.right-text{
  margin-right: 32rpx;
}
.add-btn{
  height: 24px;
  line-height: 22px;
  margin-top: 1px;
}

.cont{
  padding: 0 0 16rpx 0;
}
.title{  
  height: 90rpx;
  line-height: 90rpx;
  color: #000;
  border-bottom: 1rpx #f2f2f2 solid;
  margin:0 30rpx;
  font-size: 30rpx;
  margin-bottom: 16rpx;
}
.weui-cell::before{
  border-top: none;
}
.weui-cell__bd {
  color: #999999;
}
.weui-cell__ft {
  color: #686868;
}
.sawtooth{  
  background-image: -webkit-gradient(linear, 50% 0, 0 100%, from(transparent), color-stop(.5, transparent), color-stop(.5, #F2F2F2), to(#F2F2F2)),
  -webkit-gradient(linear, 50% 0, 100% 100%, from(transparent), color-stop(.5, transparent), color-stop(.5, #F2F2F2), to(#F2F2F2));
  background-size: 8px 5px;
  background-repeat: repeat-x;
  background-position: 0 100%;
}
.total{
  text-align: right;
  height: 100rpx;
  border-top: 1rpx #f2f2f2 solid;
  margin:0 30rpx;
}
.total-price{
  color: #ff5770;
  font-size:34rpx;
}
