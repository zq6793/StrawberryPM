.preview_panel{
  margin-top: 20rpx;
}
.tip{
  padding: 20rpx 0;
}
.check-bill{
  width: 133rpx;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
  border: 1px #614689 solid;
  border-radius: 10rpx;
  float: right;
}
.line{
  border-bottom: 1px rgb(229,229,229) solid;
  margin-left: 30rpx; 
}
.weui-panel_access,
.info{
  margin-top: 0;
}
.distance_bottom{
  width: 100%;
  height: 150rpx;
  margin: 40rpx auto;
}
.img_distance{
  margin: 0 auto;
}
.resident_document{
  width:150rpx;
  height: 150rpx;
  text-align: center;
  line-height: 150rpx;
  border: 1rpx solid #d9d9d9;
} 
.resident_document image{
  width:150rpx;
  height: 150rpx;
}
.sub{
  font-size: 10px;
  position: relative;
  top: -10rpx;
}
.pic-cont{
  padding: 10rpx;
  margin: 0 auto;
}
.pic-cont .pic{
  width: 140rpx;
  height: 140rpx;
  border: 1px #f2f2f2 solid;
  text-align: center;
  margin:7px;
}

.weui-cells::after,
.weui-form-preview__hd::after,
.weui-form-preview::after,
.weui-panel::after{
	border-bottom: none;
}
.weui-form-preview::before,
.weui-panel::before{
  border-top: none;
}