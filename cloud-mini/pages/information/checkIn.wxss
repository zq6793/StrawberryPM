.weui-btn{
	width: 690rpx;
	height: 88rpx;
	margin: 50rpx auto;
}
.btn[type="primary"]{ 
	background-color: #614789;
}
.weui-btn:first-child{
  margin-top: 50rpx;
}
.font_color{
  color: rgba(0,0,0,0.5)
}
.check-in-list{
	height: 80rpx;
	line-height: 80rpx;
}
.name{
	width: 154rpx;
}
.info{
	text-align: left;
}
.title{
	height: 80rpx;
	border-top: none;
}
.coupon{
	margin-top: 10px;
}
.coupon .weui-check__label:first-child{
	border-top: 1px #ededed solid;
}
.distance_bottom{
  width: 100%;
  height: 150rpx;
  margin: 40rpx auto;
}
.img_distance{
  font-size: 28rpx;
  margin: 0 auto;
  text-align: center;
}
.resident_document,
.resident_documents {
  width:150rpx;
  height: 150rpx;
  line-height: 150rpx;
  border: 1rpx solid #d9d9d9;
} 
.resident_document image,
.resident_documents image{
  width:150rpx;
  height: 150rpx;
}
.img_distances{
  margin-left: 20rpx;
}
.add_pic{
  width:150rpx;
  height: 150rpx;
  line-height: 150rpx;
  border: 1px #ededed solid;
  text-align: center;
  float:left;
}
.room-picture{
  padding: 0 20rpx 10rpx 30rpx;
}
.tips{
  padding: 0 30rpx;
  margin: -10rpx 0 10rpx 0;
}

.weui-cells::after{
  border-bottom: none;
}
.weui-cells::before{
  border-top: none;
}